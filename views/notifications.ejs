<%- include("header") -%>
<%for(var i = 0 ; i<notifArray.length ; i++){%>
<%if(i%2==0){%>
<div class="container-fluid" id="notifs" style="text-align:left; background-color:#112d4e;">
  <div class="row" style="padding:30px;">
    <div class="col-4">
      <embed src=<%=notifArray[i].link%> width="300px" height="210px" />
    </div>
    <div class="col-8">
      <small id="emailHelp" class="form-text date" style="color:#112d4e;">  <%=notifArray[i].date%></small>
      <p style="color:#dbe2ef"><%=notifArray[i].title%></p>
    </div>
  </div>
</div>
<%}else{%>
<div class="container-fluid" id="notifs" style="text-align:right;background-color:#dbe2ef">
  <div class="row" style="padding:30px;">
    <div class="col-8">
      <small id="emailHelp" class="form-text date" style="background-color:#112d4e;">  <%=notifArray[i].date%></small>
      <p style="color:#112d4e;"><%=notifArray[i].title%></p>
    </div>
    <div class="col-4">
      <embed src=<%=notifArray[i].link%> width="300px" height="210px" />
    </div>
  </div>
</div>
<%}%>
<%}%>

<%- include("footer") -%>
